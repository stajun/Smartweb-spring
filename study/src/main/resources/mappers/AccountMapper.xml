<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.green.spring.dao.AccountDao">
  <select id="getAccount" resultType="kr.green.spring.vo.AccountVo">
    select * 
    	from account 
    	where id = #{id}
  </select>
  <insert id="setAccount">
  	insert into 
  		account(id, pw, email, gender, authority)
  		values(#{accountVo.id},#{accountVo.pw},#{accountVo.email},
  		       #{accountVo.gender},#{accountVo.authority})
  </insert>
</mapper>